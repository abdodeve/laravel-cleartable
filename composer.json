{
    "name": "abdodeve/clear-table",
    "description": "Clean any table by deleting all rows with a simple artisan commande",
    "keywords":
    [
        "clear",
        "delete",
        "clean",
        "purge",
        "laravel"
    ],
    "authors": [
        {
            "name": "Abdelhadi Habchi",
            "email": "abhabchi@sqli.com"
        }
    ],
    "require": {
        "php" : "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Abdodeve\\ClearTable\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "license": "MIT",
    "extra": {
        "laravel": {
            "providers": [
                "Abdodeve\\ClearTable\\ClearTableServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
